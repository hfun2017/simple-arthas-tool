{
    "name": "simple-arthas-tool",
    "displayName": "Simple Arthas Tool",
    "icon": "images/icon.png",
    "description": "根据当前的Java字段，生成对应的Arthas命令并复制到剪切板",
    "version": "1.0.0",
    "publisher": "hanfang",
    "author": {
        "name": "hanfang"
    },
    "maintainers": [
        "hanfang@yonyou.com"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/hfun2017/simple-arthas-tool"
    },
    "engines": {
        "vscode": "^1.33.0"
    },
    "categories": [
        "Other",
        "Debuggers"
    ],
    "keywords": [
        "arthas",
        "java",
        "debug",
        "trace",
        "watch"
    ],
    "activationEvents": [
        "onLanguage:java"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "simple-arthas-tool.watch",
                "title": "Arthas Watch"
            },
            {
                "command": "simple-arthas-tool.trace",
                "title": "Arthas Trace"
            },
            {
                "command": "simple-arthas-tool.sc",
                "title": "Arthas Search-Class"
            },
            {
                "command": "simple-arthas-tool.jad",
                "title": "Arthas Decompile"
            },
            {
                "command": "simple-arthas-tool.stack",
                "title": "Arthas Stack"
            }
        ],
        "menus": {
            "editor/context": [
                {
                    "when": "editorLangId == java",
                    "submenu": "simple-arthas-tool.arthasSubmenu",
                    "group": "navigation@100"
                }
            ],
            "simple-arthas-tool.arthasSubmenu": [
                {
                    "command": "simple-arthas-tool.watch",
                    "group": "1_arthas@1"
                },
                {
                    "command": "simple-arthas-tool.trace",
                    "group": "1_arthas@2"
                },
                {
                    "command": "simple-arthas-tool.stack",
                    "group": "1_arthas@3"
                },
                {
                    "command": "simple-arthas-tool.sc",
                    "group": "1_arthas@4"
                },
                {
                    "command": "simple-arthas-tool.jad",
                    "group": "1_arthas@5"
                }
            ]
        },
        "submenus": [
            {
                "id": "simple-arthas-tool.arthasSubmenu",
                "label": "Arthas 工具"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "lint": "eslint . --ext .ts,.tsx",
        "watch": "tsc -watch -p ./"
    },
    "devDependencies": {
        "@types/node": "^14.14.10",
        "@types/vscode": "^1.100.0",
        "@typescript-eslint/eslint-plugin": "^3.0.2",
        "@typescript-eslint/parser": "^3.0.2",
        "@vscode/vsce": "^3.5.0",
        "eslint": "^7.1.0",
        "typescript": "^4.0.2"
    }
}
